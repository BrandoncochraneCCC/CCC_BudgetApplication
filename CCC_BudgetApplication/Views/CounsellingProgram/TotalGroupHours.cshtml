@model List<Application.ViewModels.DataTable>
@{
    ViewBag.Title = "CounsellingHours";
}
<h2> @Html.ActionLink("Total Group Hours", "Index", "Counselling")</h2>
<hr />
@{
    foreach (var item in Model)
    {
        <h4>
            @if (item.tableName != null && item.tableName != "")
            {
                @Html.ActionLink(item.tableName, "TotalGroupHours", "CounsellingProgram", new { ID = item.sourceID }, null)

            }
            else
            {
                @item.tableName
            }
        </h4>
        <table class="table">

            @Html.Partial("~/Views/Shared/_NoNameTableHeader.cshtml")
            @Html.Partial("~/Views/Shared/_DataTable.cshtml", item)
        </table>

    }
}

<h2>Totals</h2>
<table class="table totalTable">
    <tr class="allTableSum">

        <td class="name"><strong>Total</strong></td>
        <td class="pageTotal"></td>

        @for (var i = 1; i < 13; i++)
        {
            var column = "colTotal" + i;
            <td class="@column"></td>
        }

    </tr>
</table>



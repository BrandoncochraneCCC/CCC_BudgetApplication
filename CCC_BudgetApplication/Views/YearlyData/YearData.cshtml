
@{
    ViewBag.Title = "YearData";
}

<h2>@ViewBag.Year Data</h2>
@using (Html.BeginForm("DeductionListRate", "YearlyData", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="oldValue" value="test" />

    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group col-md-3">
            @Html.Editor("newValue", new { htmlAttributes = new { @Class = "form-control", @Value = "test" } })
            @Html.ValidationMessage("value", "", new { @class = "text-danger" })

        </div>
    </div>

        <div class="form-group col-md-2">
            <div class="">
                <input type="submit" name="DeductionListRate" value="Create" class="btn btn-default" />
            </div>
        </div>
        <div class="col-md-6"></div>
}

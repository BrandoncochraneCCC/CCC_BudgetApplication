@model List<Application.ViewModels.DataTable>
@{
    ViewBag.Title = "Targets";
}

<h2>Targets</h2>
<hr />
@{
    foreach (var item in Model)
    {
        if (item != null)
        {
            <h4>@Html.ActionLink(item.tableName, "EmployeeData", "Employees", new { ID = item.sourceID }, null)</h4>
            <table class="table">
                @Html.Partial("~/Views/Employees/Data/TargetTable.cshtml", item)
            </table>
            @Html.Partial("~/Views/Shared/_EmptyLine.cshtml")
        }
    }
}


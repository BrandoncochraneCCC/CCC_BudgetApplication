@model string

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="Type" value="@Model" />

    <div class="form-horizontal">
        <p>Create @Model:</p>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group col-md-3">
                @Html.Editor("value", new { htmlAttributes = new { @class = "form-control", @name = @Model, @placeholder = "value" } })
                @Html.ValidationMessage("value", "", new { @class = "text-danger" })
               
            </div>
        </div>

        <div class="form-group col-md-2">
            <div class="">
                <input type="submit" name="benefit" value="Create" class="btn btn-default" />
            </div>
        </div>
        <div class="col-md-6"></div>
}

